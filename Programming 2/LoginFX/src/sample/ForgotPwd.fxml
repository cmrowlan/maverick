<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="202.0" prefWidth="350.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.ForgotPwdController">
   <children>
      <Label layoutX="42.0" layoutY="14.0" text="Forgot Password">
         <font>
            <Font size="36.0" />
         </font>
      </Label>
      <Label layoutX="22.0" layoutY="59.0" text="Email" />
      <TextField fx:id="txtEmail" layoutX="22.0" layoutY="80.0" prefHeight="25.0" prefWidth="306.0" />
      <Button fx:id="btnSend" layoutX="42.0" layoutY="145.0" mnemonicParsing="false" onAction="#send" text="Send" />
      <Button fx:id="btnCancel" layoutX="256.0" layoutY="145.0" mnemonicParsing="false" onAction="#cancel" text="Cancel" />
      <Label fx:id="lblError" layoutX="63.0" layoutY="117.0" prefHeight="17.0" prefWidth="231.0" textAlignment="CENTER" />
   </children>
</AnchorPane>
