<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="350.0" prefWidth="350.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.ChangePasswordController">
   <children>
      <Label layoutX="34.0" layoutY="14.0" text="Change Password">
         <font>
            <Font size="36.0" />
         </font>
      </Label>
      <Label layoutX="39.0" layoutY="80.0" text="Current Password" />
      <PasswordField fx:id="txtCurrentPwd" layoutX="39.0" layoutY="97.0" prefHeight="25.0" prefWidth="270.0" />
      <Label layoutX="39.0" layoutY="135.0" text="New Password" />
      <PasswordField fx:id="txtNewPwd" layoutX="39.0" layoutY="152.0" prefHeight="25.0" prefWidth="270.0" />
      <Label layoutX="39.0" layoutY="192.0" text="Confirm New Password" />
      <PasswordField fx:id="txtCNewPwd" layoutX="39.0" layoutY="209.0" prefHeight="25.0" prefWidth="270.0" />
      <Button layoutX="39.0" layoutY="264.0" mnemonicParsing="false" onAction="#change" text="Change Password" />
      <Button fx:id="btnCancel" layoutX="257.0" layoutY="264.0" mnemonicParsing="false" onAction="#cancel" text="Cancel" />
   </children>
</AnchorPane>
